<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingStart="14dp"
    android:paddingEnd="14dp"
    android:descendantFocusability="blocksDescendants">

    <Button
        android:id="@+id/to_do_item_main_item_icon"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:focusable="false"
        android:clickable="true"
        android:background="@drawable/delete_label_select"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        android:id="@+id/to_do_item_main_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="30dp"
        android:textSize="18dp"
        android:textColor="#333"/>
    <Button
        android:id="@+id/to_do_item_main_icon2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/to_do_item_main_icon2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <!--<Button-->
        <!--android:id="@+id/to_do_item_main_cancel_icon2"-->
        <!--android:layout_width="20dp"-->
        <!--android:layout_height="20dp"-->
        <!--android:background="@drawable/to_do_item_main_cancel_icon"-->
